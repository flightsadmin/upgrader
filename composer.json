{
    "name": "flightsadmin/upgrader",
    "description": "Upgrade CodeIgniter 3 to CodeIgniter 4",
    "keywords": [
        "flightsadmin",
        "upgrader"
    ],
    "homepage": "https://github.com/flightsadmin/upgrader",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "George Chitechi",
            "email": "george@flightadmin.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Flightsadmin\\Upgrader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flightsadmin\\Upgrader\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
